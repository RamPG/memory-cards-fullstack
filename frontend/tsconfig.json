{
  "compilerOptions": {
    "target": "es2017",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "module": "esnext",
    "noEmit": false,
    "jsx": "react"
  },
  "exclude": [
    "dist"
  ],
  "include": [
    "./src/**/*"
  ]
}
